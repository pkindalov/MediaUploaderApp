<h1>Upload New File</h1>
<%= form_with model: [@folder, @media_file], local: true do |form| %>
  <div class="mb-3">
    <%= form.label :file, class: 'form-label' %>
    <%= form.file_field :file, class: 'form-control' %>
  </div>
  <%= form.submit 'Upload File', class: 'btn btn-primary' %>
<% end %>
